params {
  project = 'migraine_EUR_chr22_burden'
  outdir = "${WORKSPACE_BUCKET}/results/migraine_EUR_chr22_burden"
  genotypes_array = "${WORKSPACE_BUCKET_GWAS_MIGRAINE}/arrays/EUR/arrays_EUR_qcfiltered.{bim,bed,fam}"
  genotypes_bgen = "gs://fc-aou-datasets-controlled/v8/wgs/short_read/snpindel/exome/bgen/chr22.bgen"
  sample_file = "gs://fc-aou-datasets-controlled/v8/wgs/short_read/snpindel/acaf_threshold/bgen/chr1.sample"
  phenotypes_files = "${WORKSPACE_BUCKET_GWAS_MIGRAINE}/phenotypes/migraine_eur.tsv"
  phenotypes_binary_trait = true
  covariates_file = "${WORKSPACE_BUCKET_GWAS_MIGRAINE}/covariates.tsv"

  regenie_test        = 'additive'
  regenie_bsize_step1 = 1000
  regenie_bsize_step2 = 400
  regenie_firth       = false
  regenie_firth_approx = false

  regenie_gene_masks                    = "${WORKSPACE_BUCKET_GWAS_MIGRAINE}/burden/masks.tsv"
  regenie_gene_setlist                  = "${WORKSPACE_BUCKET_GWAS_MIGRAINE}/burden/set_list.tsv"
  regenie_gene_anno                     = "${WORKSPACE_BUCKET_GWAS_MIGRAINE}/burden/annotation.tsv"

  regenie_gene_aaf     = "0.001,0.1"
}




process {
    withName: '.*' {
        cpus = 8
        memory = 16.GB
        machineType = 'n2-standard-8'
    }
}

// includeConfig 'conf/aou_burden.config'

process.container = "${ARTIFACT_REGISTRY_DOCKER_REPO}/florianzink/nf-gwas-gcloud:v0.3"
